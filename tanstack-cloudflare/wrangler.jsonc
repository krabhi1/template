{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "tanstack-cloudflare-template",
  "compatibility_date": "2025-09-02",
  "compatibility_flags": ["nodejs_compat"],
  "main": "src/server/entry-worker.ts",
  "observability": {
    "enabled": true,
  },

  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "tanstack-cloudflare-template",
      "database_id": "demo",
    },
  ],
  "r2_buckets": [{ "bucket_name": "demo", "binding": "R2" }],
}
